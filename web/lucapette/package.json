    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.3",
    "lucide-static": "^0.563.0",
    "playwright": "^1.58.2",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.2.5",
    "stylelint": "^17.3.0"
  }
